  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "cookie-session": "^1.3.3",
    "express": "^4.17.0",
    "moment": "^2.24.0",
    "mongoose": "^5.5.9",
    "nodemon": "^1.19.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0"
  }
}
